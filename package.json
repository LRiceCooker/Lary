{
  "name": "lary",
  "version": "0.0.12-0",
  "description": "A utility-first style library for React Native on top of Dripsy.",
  "scripts": {
    "doc": "jsdoc2md --files ./src/**/*.tsx ./src/**/**/*.tsx --configure ./jsdoc2md.json > ./doc/doc.md",
    "rollup": "rollup -c",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "release": "npm version",
    "prepatch": "npm version prepatch",
    "preminor": "npm version preminor",
    "premajor": "npm version premajor",
    "prerelease": "npm version prerelease",
    "playground:generate": "node -e 'require(\"./playground\").generate()'",
    "playground:run": "node -e 'require(\"./playground\").run()'"
  },
  "keywords": [],
  "author": "RiceCooker",
  "license": "Beerware",
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "dripsy": "^3.6.0",
    "react-native-svg": "^12.1.1",
    "rollup": "^2.73.0",
    "rollup-plugin-dts": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jsdoc-to-markdown": "^7.0.3",
    "@types/node": "^17.0.35",
    "@types/react-native": "^0.67.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^8.0.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "prettier": "^2.6.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}
