{
  "name": "@_ricecooker/lary",
  "version": "0.4.0",
  "description": "A utility-first style library for React Native on top of Dripsy.",
  "repository": {
    "type": "git",
    "url": "https://github.com/RiceCooker-dev/Lary.git"
  },
  "homepage": "https://github.com/RiceCooker-dev/Lary/tree/stable",
  "private": false,
  "scripts": {
    "doc": "jsdoc2md --files ./src/**/*.tsx ./src/**/**/*.tsx --configure ./jsdoc2md.json > ./doc/doc.md",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "prepare": "npx husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "release": "npm version",
    "prepatch": "npm version prepatch",
    "preminor": "npm version preminor",
    "premajor": "npm version premajor",
    "prerelease": "npm version prerelease",
    "playground:generate": "node -e 'require(\"./playground\").generate()'",
    "playground:run": "node -e 'require(\"./playground\").run()'"
  },
  "keywords": [],
  "author": "RiceCooker",
  "license": "Beerware",
  "dependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "dripsy": "^4.0.2",
    "javascript-color-gradient": "^2.4.4",
    "lodash-es": "^4.17.21",
    "react-native-svg": "^13.9.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/javascript-color-gradient": "^2.4.0",
    "@types/jsdoc-to-markdown": "^7.0.3",
    "@types/lodash": "^4.14.192",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@types/react-native": "^0.71.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "prettier": "^2.8.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}
